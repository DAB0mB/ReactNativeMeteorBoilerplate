{
  "name": "ReactNativeMeteorBoilerplate",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "jest-expo": "~27.0.0",
    "meteor-client-bundler": "git+https://github.com/ardatan/meteor-client-bundler.git",
    "react-native-scripts": "1.14.0",
    "react-test-renderer": "16.3.1"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "run-android": "react-native-scripts android",
    "run-ios": "react-native-scripts ios",
    "test": "jest",
    "api": "cd api && meteor run",
    "meteor-bundle-client": "meteor-client bundle -s api",
    "postinstall": "yarn meteor-bundle-client"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.52",
    "expo": "^27.0.1",
    "meteor-node-stubs": "^0.4.1",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "react-native-meteor-polyfills": "^1.0.5"
  }
}